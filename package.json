{
  "name": "nodejs-app",
  "version": "1.0.0",
  "description": "A node.js application boilerplate writing in ES6, Babel and ESLint",
  "main": "build/index.js",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node",
    "lint": "eslint src",
    "build": "rm -rf build && babel src -d build",
    "serve": "nodemon build/index.js",
    "api": "./node_modules/.bin/babel-node ./src/index.js",
    "download-apk": "./node_modules/.bin/babel-node ./src/download-apk.js",
    "install-ios-app": "./node_modules/.bin/babel-node ./src/install-ios-app.js",
    "install-ios-app-wdio": "./node_modules/.bin/babel-node ./src/install-ios-app-wdio.js",
    "ios-apps": "./node_modules/.bin/babel-node ./src/ios-apps",
    "trust-devices": "./node_modules/.bin/babel-node ./src/find-trust-devices.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ledinhphuong/nodejs-es6-babel-eslint.git"
  },
  "keywords": [
    "nodejs",
    "es6",
    "babel",
    "eslint",
    "boilerplate"
  ],
  "author": "Phuong Le",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ledinhphuong/nodejs-es6-babel-eslint/issues"
  },
  "homepage": "https://github.com/ledinhphuong/nodejs-es6-babel-eslint#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.4.2",
    "babel-eslint": "^8.2.6",
    "bluebird": "^3.5.4",
    "colors": "^1.4.0",
    "eslint": "^5.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "gpapi": "^4.5.0",
    "http-status-codes": "^1.3.2",
    "nodemon": "^1.18.10",
    "wd": "^1.11.4",
    "webdriverio": "^5.16.3"
  },
  "dependencies": {
    "request": "^2.88.0"
  }
}
